urxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed

! URxvt.backgroundPixmap: /home/sven/.urxvt/urxvtpixmap.png;style=tiled
URxvt.lineSpace: 0
URxvt.internalBorder: 15
URxvt.scrollBar: false
! URxvt.font: xft:azuki_font:pixelsize=17
! URxvt.boldFont: xft:azukifontB:style=bold:pixelsize=17
! URxvt.font: xft:iosevka:style=regular:pixelsize=17
! URxvt.font: xft:CactusDOSVGA437Win:style=regular:pixelsize=17
URxvt.font: xft:CozetteVector-Regular:style=regular:pixelsize=17
URxvt.boldFont: xft:iosevka:style=bold:pixelsize=17
URxvt.intensityStyles: false
URxvt.depth: 32

XTerm.vt100.faceName: Fira Code Mono:style=regular:size=10:antialias=true

*termName: xterm-256color
*internalBorder: 6
