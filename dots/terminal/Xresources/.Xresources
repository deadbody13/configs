urxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed

URxvt.backgroundPixmap: /home/sven/.urxvt/urxvtpixmap.png;style=tiled
URxvt.lineSpace: 0
URxvt.internalBorder: 15
URxvt.scrollBar: false
URxvt.font: xft:azuki_font:pixelsize=17
URxvt.boldFont: xft:azukifontB:style=bold:pixelsize=17
URxvt.intensityStyles: false

XTerm.vt100.faceName: Fira Code Mono:size=10:antialias=true

*termName: xterm-256color
*internalBorder: 6

! special
*.foreground:   #93a1a1
*.background:   #002b36
*.cursorColor:  #93a1a1

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3


